{
  "name": "ng-material-poc",
  "version": "0.0.6",
  "scripts": {
    "ci": "npm ci",
    "ng": "ng",
    "start": "npx concurrently \"npm run server\" \"npm run serve\"",
    "start:ssl": "npx concurrently \"npm run server\" \"npm run serve:ssl\"",
    "serve": "ng serve --host 0.0.0.0 --disable-host-check",
    "serve:ssl": "ng serve --host 0.0.0.0 --ssl true --ssl-key localhost-key.pem --ssl-cert localhost.pem",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "server": "npx json-server --watch db.json",
    "http-server": "npx http-server mock-server/videos",
    "mock-server-post": "node mock-server/server.js",
    "ngrok": "npx ngrok http --host-header=rewrite 4200",
    "clean:videos": "npx rimraf mock-server/videos/*",
    "mkcert": "echo 'install mkcert like: brew install mkcert' && echo 'mkcert -install' && echo 'mkcert localhost' && exit 1"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.1.0",
    "@angular/cdk": "^15.2.2",
    "@angular/common": "^15.1.0",
    "@angular/compiler": "^15.1.0",
    "@angular/core": "^15.1.0",
    "@angular/forms": "^15.1.0",
    "@angular/material": "^15.2.2",
    "@angular/platform-browser": "^15.1.0",
    "@angular/platform-browser-dynamic": "^15.1.0",
    "@angular/router": "^15.1.0",
    "@ffmpeg/core": "^0.11.0",
    "@ffmpeg/ffmpeg": "^0.11.6",
    "animate.css": "^4.1.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-winston": "^4.2.0",
    "js-ffmpeg": "^0.0.38",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^4.0.2",
    "ramda": "^0.29.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "winston": "^3.9.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.1.5",
    "@angular/cli": "~15.1.5",
    "@angular/compiler-cli": "^15.1.0",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^20.2.5",
    "@types/ramda": "^0.29.2",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "pnpm": "^8.6.1",
    "typescript": "~4.9.4"
  }
}
