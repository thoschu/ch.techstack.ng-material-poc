{
  "$schema": "../../node_modules/@angular/service-worker/config/schema.json",
  "index": "/src/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/src/favicon.ico",
          "/src/index.html",
          "/src/manifest.webmanifest",
          "/src/*.css",
          "/src/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/src/assets/**",
          "/src/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    }
  ]
}
